pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Hello Checkout'
                checkout scm
            }
        }
        stage('Config') {
            steps {
                echo 'Hello Config'
            }
        }
        stage('Ci') {
            steps {
                echo 'Hello Ci'
            }
        }
        stage('Build') {
            steps {
                echo 'Hello Build'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello Test'
            }
        }
        stage('Archive') {
            steps {
                echo 'Hello Archive'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello Deploy'
            }
        }
    }
}

